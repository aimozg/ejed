    <!-- ********
         COMMANDS
         ******** -->
    <!-- stats -->
    <!-- public function DrainStat(statname:String, value:Number):void -->
    <command name="DrainStat">
        <listname>Player: Drain stat</listname>
        <param name="Statname" type="BuffableStatName" default="'str'"/>
        <param name="Value" type="float" default="5"/>
        <editor>
            Drain
            <param name="Value"/>
            points of player's
            <param name="Statname" type="SELECT"/>
        </editor>
        <editor-hint>
            Will create or increase "Drain" debuff.
            Negative values would reduce debuff instead, however, "Player: Recover drained stat" command is preferred.
        </editor-hint>
    </command>
    <!-- public function RecoverStat(statname:String, value:Number):void -->
    <command name="RecoverStat">
        <listname>Player: Recover drained stat</listname>
        <param name="Statname" type="BuffableStatName" default="'str'"/>
        <param name="Value" type="float" default="5"/>
        <editor>
            Recover up to
            <param name="Value"/>
            points of player's
            <param name="Statname" type="SELECT"/>
        </editor>
        <editor-hint>
            Will reduce "Drain" debuff, removing if it goes to 0.
            Negative values would increase debuff instead, however, "Player: Drain stat" command is preferred.
        </editor-hint>
    </command>
    <!-- public function BuffOrRecover(statname:String, value:Number, tag:String, text:String, tick: int, rate:int):void { -->
    <command name="BuffOrRecover">
        <listname>Player: Buff/Recover stat</listname>
        <param name="Statname" type="BuffableStatName" default="'str'"/>
        <param name="Value" type="float" default="5"/>
        <param name="Tag" type="string" default="''"/>
        <param name="Text" type="string" default="'Bonus'"/>
        <param name="Tick" type="int" default="36"/>
        <param name="Rate" type="TempBuffRate" default="2"/>
        <editor>
            Buff/Recover player's
            <param name="Statname" type="SELECT"/>
            stat by
            <param name="Value"/>
            for
            <param name="Tick" type="INPUT"/>
            <param name="Rate" type="SELECT"/>
            \n with tag
            <param name="Tag" type="INPUT"/>
            and visible text
            <param name="Text" type="INPUT"/>
        </editor>
        <editor-hint>
            If player has "Drain" debuff applied, stat will be recovered by the value specified.
            Otherwise, add temporary buff. Negative values are not allowed.
            Existing buff for same stat with same tag will be replaced.
            Recommended tag name is 'context name/buff name', e.g. "templeOfAimozg/superBlessing"
        </editor-hint>
    </command>
    <command name="Buff">
        <listname>Creature: Add temporary buff</listname>
        <param name="Creature" type="Creature" default="player"/>
        <param name="Statname" type="BuffableStatName" default="'str'"/>
        <param name="Value" type="float" default="5"/>
        <param name="Tag" type="string" default="''"/>
        <param name="Text" type="string" default="'Bonus'"/>
        <param name="Tick" type="int" default="36"/>
        <param name="Rate" type="TempBuffRate" default="2"/>
        <editor>
            Buff
            <param name="Creature"/>'s
            <param name="Statname" type="SELECT"/>
            stat by
            <param name="Value"/>
            for
            <param name="Tick" type="INPUT"/>
            <param name="Rate" type="SELECT"/>
            \n with tag
            <param name="Tag" type="INPUT"/>
            and visible text
            <param name="Text" type="INPUT"/>
        </editor>
        <editor-hint>
            Existing buff for same stat with same tag will be increased.
            Negative values are allowed.
            Recommended tag name is 'context name/buff name', e.g. "templeOfAimozg/superBlessing"
        </editor-hint>
    </command>
    <command name="BuffClearByTagStat">
        <listname>Creature: Clear specific buff from stat</listname>
        <param name="Creature" type="Creature" default="player"/>
        <param name="Statname" type="BuffableStatName" default="'str'"/>
        <param name="Tag" type="string" default="''"/>
        <editor>
            Remove buff tagged
            <param name="Tag" type="INPUT"/>
            from
            <param name="Creature"/>'s
            <param name="Statname" type="SELECT"/>
            stat
        </editor>
    </command>
    <command name="BuffClearByTag">
        <listname>Creature: Clear specific buff from all stats</listname>
        <param name="Creature" type="Creature" default="player"/>
        <param name="Tag" type="string" default="''"/>
        <editor>
            Remove buffs tagged
            <param name="Tag" type="INPUT"/>
            from
            <param name="Creature"/>'s
            stats
        </editor>
    </command>
    <command name="ModRawStat">
        <listname>Creature: Modify non-buffable stat</listname>
        <param name="Creature" type="Creature" default="player"/>
        <param name="Statname" type="RawStatName" default="'lust'"/>
        <param name="Value" type="float" default="5.0"/>
        <param name="Unscaled" type="boolean" default="false"/>
        <editor>
            Modify
            <param name="Creature"/>'s
            <param name="Statname" type="SELECT"/>
            stat by
            <param name="Value"/>
            <param name="Unscaled" type="CHECKBOX" typedata="(unscaled)"/>
        </editor>
        <editor-hint>
            Positive to increase, negative to decrease. Won't go over max/min.
            "Unscaled" means that resistances/bonuses that might ater value won't be applied.
        </editor-hint>
    </command>
